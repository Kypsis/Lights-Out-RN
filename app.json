{
  "expo": {
    "name": "Lights On",
    "description": "Classical Lights Out/On game",
    "slug": "LightsOn",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#090C22"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "primaryColor": "#090C22",
